ifeq ($(SDK_CONFIG), $(wildcard $(SDK_CONFIG)))
include $(SDK_CONFIG)
endif

EXTRA_CFLAGS += -Wall -Werror -Wuninitialized -Wstrict-prototypes

ifeq ($(CONFIG_SDK_DRIVER_MX),m)
EXTRA_CFLAGS += -mlong-calls -fno-pic 
endif

ifneq ($(CONFIG_SDK_LINKED_KMOD),y)
