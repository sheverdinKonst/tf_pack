#ifdef CMD_TIME_GET_PTP_PORT_PORTS_ALL_STATE
//time configuration
//get configuration
//precision time protocol
//port configuration
//port list
//all ports
//state configuration
time get ptp port ( <PORT_LIST:ports> | all ) state
#endif

#ifdef CMD_TIME_SET_PTP_PORT_PORTS_ALL_STATE_ENABLE_DISABLE
