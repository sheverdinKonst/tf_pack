#ifdef CMD_UART1_GET_CHARACTER_NUMBER_TIMEOUT_TIMEOUT
//uart1 configuration
//get configuration
//character configuration
//number of input character
//timeout configuration
//timeout value (unit: milli-second); 0 mean no timeout
uart1 get character <UINT:number> timeout <UINT:timeout>
#endif

#ifdef CMD_UART1_PUT_CHARACTER_STRING
//uart1 configuration
//put configuration
//character configuration
//put string
uart1 put character <STRING:string>
#endif

#ifdef CMD_UART1_GET_BAUDRATE
//uart1 configuration
//get configuration
//baudrate configuration
uart1 get baudrate
#endif

#ifdef CMD_UART1_SET_BAUDRATE_9600_19200_38400_57600_115200
//uart1 configuration
//set configuration
//baudrate configuration
//baudrate 9600
//baudrate 19200
//baudrate 38400
//baudrate 57600
//baudrate 115200
uart1 set baudrate ( 9600 | 19200 | 38400 | 57600 | 115200 )
#endif
